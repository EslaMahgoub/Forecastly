
<%= simple_form_for @user do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.input :country, priority: [ "Poland" ] %>
  <br/>
  <%= f.button :submit, "Save", class:"btn btn-primary m-2 "%>

<% end %>